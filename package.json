{
  "name": "my-project",
  "version": "2.1.3",
  "private": true,
  "main": "./dist/main.js",
  "scripts": {
    "electron-dev": "cross-env NODE_ENV=development electron electron-main/main.js",
    "electron build": "vue-cli-service build && webpack --config=electron-main/build.js && electron-builder",
    "webpack electron-main": "webpack --config=electron-main/build.js",
    "dev": "cross-env NODE_ENV=development vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "vue": "2.6.10",
    "vue-router": "3.0.3",
    "vue-view-lazy": "0.0.6",
    "axios": "0.19.0",
    "echarts": "4.2.1",
    "element-ui": "2.11.1",
    "qs": "6.7.0"
  },
  "devDependencies": {
    "@vue/cli-service": "3.11.0",
    "vue-template-compiler": "2.6.10",
    "cross-env": "5.2.0",
    "electron": "6.0.4",
    "electron-updater": "4.1.2",
    "electron-debug": "3.0.1",
    "electron-builder": "21.2.0",
    "node-native-loader": "1.1.1",
    "webpack-cli": "3.3.7"
  },
  "vue": {
    "publicPath": "./",
    "outputDir": "dist",
    "assetsDir": "",
    "lintOnSave": false,
    "runtimeCompiler": true,
    "productionSourceMap": false,
    "parallel": true,
    "pwa": {}
  },
  "build": {
    "productName": "股票行情观察",
    "appId": "StockInfoWatch",
    "directories": {
      "output": "build"
    },
    "publish": [
      {
        "provider": "github",
        "url": "https://github.com",
        "owner": "guoyucode",
        "repo": "StockInfoWatch-package",
        "private": false
      }
    ],
    "files": [
      "dist/**"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "public/icons/icon.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/icons/icon.ico"
    },
    "linux": {
      "icon": "public/icons/icons"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
